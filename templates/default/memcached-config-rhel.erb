# Memcached config for <%= @instance_name %>
<% if @config.bind_ip %>
BIND_IP="<%= @config.bind_ip %>"
<% end %>
<% if @config.port %>
PORT="<%= @config.port %>"
<% end %>
<% if @config.service_user %>
USER="<%= @config.service_user %>"
<% end %>
<% if @config.max_connections %>
MAXCONN="<%= @config.max_connections %>"
<% end %>
<% if @config.max_memory %>
CACHESIZE="<%= @config.max_memory %>"
<% end %>
OPTIONS="<% if @config.udp_port %>-U <% @config.udp_port %><% end %> <% if @config.threads %> -t <%= @config.threads %><% end %> <% if @config.max_object_size %> -I <%= @config.max_object_size %><% end %> <% if @config.options %> <%= @config.options %><% end %>"
